/*CREACION DE BASE DE DATOS*/
CREATE DATABASE nombre;

/*ELIMINACION DE BASE DE DATOS*/
DROP DATABASE nombre;

/*MOSTRAR BASES DE DATOS*/
SHOW DATABASES;

/*USAR BASE DE DATOS*/
USE DATABASE nombre;

/*CREACION DE TABLAS*/
CREATE TABLE nombre(nomAttr1 INTEGER, nomAttr2 INTEGER);

/*ELIMINACION DE TABLAS*/
DROP TABLE nombre;

/*MOSTRAR TABLAS*/
SHOW TABLES;

/* INSERCION DE REGISTROS */
INSERT INTO nombreTabla VALUES(1.5 , "okay", false);

/* ELIMINACION DE REGISTROS */
DELETE FROM nombreTabla WHERE nomAttr = 1.6;

/* MODIFICACION DE REGISTROS */
UPDATE nombreTabla SET nomAttr = true;
UPDATE nombreTabla SET nomAttr = 4 WHERE nomAttr1 = 3;

/*MOSTRAR DE REGISTROS */
SELECT nomAttr, nomAttr FROM nombreTabla;
SELECT * FROM nombreTabla;
SELECT nomAttr, nomAttr FROM nombreTabla WHERE ID = 0.6;
SELECT * FROM nombreTabla WHERE ID = 5;
