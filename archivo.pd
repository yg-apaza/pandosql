/*CREACION DE BASE DE DATOS*/
CREATE DATABASE nombre_de_BD;

/*ELIMINACION DE BASE DE DATOS*/
DROP DATABASE nombre_de_BD;

/*MOSTRAR BASES DE DATOS*/
SHOW DATABASES;

/*USAR BASE DE DATOS*/
USE DATABASE nombre_de_BD;

/*CREACION DE TABLAS*/
CREATE TABLE nombre_de_tabla(nomAttr1 INTEGER, nomAttr2 INTEGER);

/*ELIMINACION DE TABLAS*/
DROP TABLE nombre_de_tabla;

/*MOSTRAR TABLAS*/
SHOW TABLES;

/* INSERCION DE REGISTROS */
INSERT INTO nombre_de_tabla VALUES(1.5 , "okay", false);

/* ELIMINACION DE REGISTROS */
DELETE FROM nombre_de_tabla WHERE nomAttr = 1.6;

/* MODIFICACION DE REGISTROS */
UPDATE nombre_de_tabla SET nomAttr = true;
UPDATE nombre_de_tabla SET nomAttr = 4 WHERE nomAttr1 = 3;

/*MOSTRAR DE REGISTROS */
SELECT atributo_1, atributo_2 FROM nombre_de_tabla;
SELECT * FROM nombre_de_tabla;
SELECT atributo_1, atributo_2 FROM nombre_de_tabla WHERE ID = 0.6;
SELECT * FROM nombre_de_tabla WHERE ID = 5;
